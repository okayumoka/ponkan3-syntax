{
  "name": "ponkan3-syntax",
  "displayName": "Ponkan3 Syntax",
  "description": "Syntax Highlight for Ponkan3",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ponkan3",
        "aliases": [
          "Ponkan3",
          "ponkan3"
        ],
        "extensions": [
          ".pon",
          "pon3"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "javascript",
        "aliases": [
          "JavaScript",
          "javascript",
          "js"
        ],
        "extensions": [
          ".js"
        ],
        "configuration": "./javascript-language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ponkan3",
        "scopeName": "source.pon3",
        "path": "./syntaxes/ponkan3.tmLanguage.json",
        "embeddedLanguages": {
          "source.js": "javascript"
        }
      },
      {
        "language": "javascript",
        "scopeName": "source.js",
        "path": "./syntaxes/JavaScript.tmLanguage.json"
      }
    ]
  }
}