{
  "name": "ponkan3-syntax",
  "displayName": "Ponkan3 Syntax",
  "description": "Syntax Highlight for Ponkan3",
  "publisher": "studiomikan",
  "version": "0.1.1",
  "license": "MIT",
  "repository": "https://github.com/okayumoka/ponkan3-syntax",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ponkan3",
        "aliases": [
          "Ponkan3",
          "ponkan3"
        ],
        "extensions": [
          ".pon",
          "pon3"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ponkan3",
        "scopeName": "source.pon3",
        "path": "./syntaxes/ponkan3.tmLanguage.json",
        "embeddedLanguages": {
          "meta.block.js.ponkan3": "javascript",
          "source.js": "javascript"
        }
      }
    ]
  }
}
